{
  "name": "pandivia",
  "version": "0.12.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "firebase:dev": "firebase emulators:start --only firestore,auth --import firebase-testdata",
    "firebase:dev:clear": "firebase emulators:start --only firestore,auth",
    "firebase:dev:export": "firebase emulators:export firebase-testdata",
    "firebase:admin": "node ./scripts/init-admin.js",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/app/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.0.1",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.1",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@angular/service-worker": "^16.0.0",
    "@ngneat/transloco": "^4.3.0",
    "@ngrx/effects": "^16.0.0",
    "@ngrx/store": "^16.0.0",
    "@ngrx/store-devtools": "^16.0.0",
    "focusable-selectors": "^0.8.0",
    "ngrx-immer": "^2.0.1",
    "rxfire": "^6.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "firebase-admin": "^11.10.1",
    "firebase-tools": "^12.4.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.2",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
